<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
  <header>
    <h1><%= title %></h1>
    <ul class="type" id="type">
      <li data-type="dot">Dot</li>
      <li data-type="column"  class="select">Column</li>
    </ul>
    <p>
      Voice <input id="volume" type="range" min="0" max="100" value="60" />
    </p>
  </header>
  <div class="left">
    <ul id="list">
      <% music.forEach(function (name) {%>
        <li title="<%= name %>"><%= name %></li>
      <% }) %>
    </ul>
  </div>
  <script type="text/javascript" src="/javascripts/MusicVisualizer.js"></script>
  <script type="text/javascript" src="/javascripts/index.js"></script>
  </body>
</html>
